package application;

import java.net.URL;
import java.util.ResourceBundle;
import javafx.fxml.FXML;
import javafx.scene.control.Button;
import javafx.scene.control.TextArea;

public class Car_comfort {

    @FXML
    private ResourceBundle resources;

    @FXML
    private URL location;

    @FXML
    private TextArea infowind;

    @FXML
    private Button inf;

    @FXML
    private Button kia;

    @FXML
    private Button scoda;

    @FXML
    private Button client_choose_car;

    @FXML
    void initialize() {
        assert infowind != null : "fx:id=\"infowind\" was not injected: check your FXML file 'Client_car(comfort).fxml'.";
        assert inf != null : "fx:id=\"inf\" was not injected: check your FXML file 'Client_car(comfort).fxml'.";
        assert kia != null : "fx:id=\"kia\" was not injected: check your FXML file 'Client_car(comfort).fxml'.";
        assert scoda != null : "fx:id=\"scoda\" was not injected: check your FXML file 'Client_car(comfort).fxml'.";
        assert client_choose_car != null : "fx:id=\"client_choose_car\" was not injected: check your FXML file 'Client_car(comfort).fxml'.";

    }
}
